<div class="container">
  <div class="row">
    <div class="span9 offset2">
      <div class="well well large">

<p>
  
  <%= image_tag(@professional.picture_url, :width => "20%") if @professional.picture.present? %>
</p>


<h3>
  <%= @professional.name %>
</h3>

<h4>
  <%= @professional.clinic %>
</h4>

<p>
  <b>Lugar de la atención:</b>
  <%= @professional.address %>
</p>



<p>
  <b>Descripción:</b>
  <%= @professional.information %>
</p>


<p>
  <b>Años de Experiencia:</b>
  <%= @professional.experience %>
</p>

<p>
  <b>Precio de Mercado:</b>
  <%= @professional.market_price %>
</p>

<p>
  <b>Ahorro:</b>
  <%= @professional.saving_price %>
</p>

<p>
  <b>Precio Final:</b>
  <%= @professional.final_price %>
