<%= simple_form_for(@professional) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :clinic %>
    <%= f.input :information %>
    <%= f.input :picture %>
    <%= f.input :experience %>
    <%= f.input :final_price %>
    <%= f.input :market_price %>
    <%= f.input :saving_price %>
    <%= f.input :longitude %>
    <%= f.input :latitude %>
    <%= f.input :gmaps %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
